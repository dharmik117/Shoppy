<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.shoppy"
    tools:ignore="ExtraText">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/shopee"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shopee"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:icon">
        <activity android:name=".Search_Activity"></activity>
        <activity android:name=".Confirm_Order" />
        <activity android:name=".Cart_Activity" />
        <activity android:name=".Product_OC" />
        <activity android:name=".ShopName_Onclick" />
        <activity
            android:name=".EditProfile"
            android:parentActivityName=".ProfilePage" />
        <activity
            android:name=".SSDetails"
            android:parentActivityName=".Setting" />
        <activity
            android:name=".changepassword"
            android:parentActivityName=".ProfilePage" />
        <activity android:name=".toolbar2" />
        <activity
            android:name=".Setting"
            android:parentActivityName=".Homepage" />
        <activity android:name=".toolbar" />
        <activity
            android:name=".Orderpae"
            android:parentActivityName=".Homepage" />
        <activity
            android:name=".Notificationpage"
            android:parentActivityName=".Homepage" />
        <activity
            android:name=".ProfilePage"
            android:parentActivityName=".Homepage" />
        <activity
            android:name=".contactus"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".splashscreen"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".registration"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".fppage"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Homepage" />
    </application>

</manifest>